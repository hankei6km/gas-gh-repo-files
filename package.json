{
  "name": "@hankei6km/gas-gh-repo-files-to-html",
  "version": "0.0.0",
  "description": "GitHub 上のリポジトリからファイル一覧を取得し HTML へ書き出す",
  "author": "hankei6km <hankei6km@gmail.com> (https://github.com/hankei6km)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/hankei6km/gas-gh-repo-files-to-html.git"
  },
  "bugs": {
    "url": "https://github.com/hankei6km/gas-gh-repo-files-to-html/issues"
  },
  "keywords": [
    "gas",
    "github",
    "html"
  ],
  "files": [
    "index.d.ts"
  ],
  "main": "",
  "type": "module",
  "types": "index.d.ts",
  "scripts": {
    "setup:clasp": "bash scripts/setup-clasp.sh && bash scripts/setup-clasprc.sh",
    "cleanup:clasp": "bash scripts/cleanup-clasp.sh",
    "clean": "npm run clean:build && npm run clean:test",
    "clean:build": "rimraf index.d.ts build/*",
    "clean:test": "rimraf test/build/*.spec.js",
    "build": "npm run clean:build && bash scripts/build.sh",
    "push": "bash scripts/clasp-with-auth.sh push --force",
    "deploy": "bash scripts/clasp-with-auth.sh deploy",
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "pretest":"bash scripts/make-test-zip.sh",
    "test:build": "npm run clean:test && npm run build && bash scripts/test-build.sh && npm run test",
    "posttest:build": "npm run clean:test",
    "lint:type-check": "tsc --noEmit"
  },
  "dependencies": {
    "chardet": "^2.0.0",
    "hast-util-raw": "^9.0.2",
    "hast-util-sanitize": "^5.0.1",
    "hast-util-to-html": "^9.0.1",
    "hastscript": "^9.0.0",
    "jszip": "^3.10.1",
    "mime": "^4.0.4",
    "url-parse": "^1.5.10"
  },
  "devDependencies": {
    "@hankei6km/files-to-licenses": "^0.1.1",
    "@types/google-apps-script": "^1.0.83",
    "@types/jest": "^29.5.12",
    "@types/url-parse": "^1.4.11",
    "esbuild": "^0.23.0",
    "jest": "^29.7.0",
    "rimraf": "^6.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5"
  }
}
